// Задача 15: Напишите программу, которая принимает на вход цифру, обозначающую день недели, и проверяет, является ли этот день выходным.
// 6 -> да
// 7 -> да
// 1 -> нет

using static System.Console;
Clear();

int day = int.Parse(ReadLine());

if (day >= 6)
{
    WriteLine("да");
}
else
{
    if (day <= 7)
    {
         WriteLine("нет");
    }
}
